<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test01"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
    <Canvas Name="MyCanvas">
      <StackPanel Name="MyStackPanel" Width="200" Margin="10">
        <Button Name="btnCheck" Content="Check"/>
        <Slider Name="sldAngle" Maximum="360"/>
        <Button Name="btnSetAngle" Content="SetAngle30"/>
        <TextBlock Name="tbAngle" Text="Angle"/>
        <Slider Name="sldExAngle" Maximum="360"/>
        <Button Name="btnSetExAngle" Content="SetAngle30"/>
        <Slider Name="sldEx2Angle" Maximum="360"/>
        <Button Name="btnSetEx2Angle" Content="TargetEx2Thumb.Angle2=30"/>
        <Button Name="btnSetRotateAngle" Content="RotateTransform.Angle=20"/>
        <TextBlock Name="tbEx2Angle" Text="Ex2Angle"/>
        <TextBlock Name="tbEx2RotateAngle" Text="Ex2RotateAngle"/>
      </StackPanel>
    </Canvas>
    </Grid>
</Window>
